const injectScript = require("injectScript");
const Object = require("Object");
const logToConsole = require("logToConsole");
const getType = require("getType");
const encodeUriComponent = require("encodeUriComponent");

// Log input data
logToConsole(data);

// Generate query params with all data except specific ones
const queryParams = Object.keys(data)
    .filter(key => ["gtmOnSuccess", "gtmOnFailure"].indexOf(key) === -1)
    .reduce((acc, key) => {
        if (key === "customParametersTable") {
            data[key].forEach(element => {
                acc.push(element.customParameter + "=" + encodeUriComponent(element.customParameterValue));
            });
        }
        else if (key === "fallbackOptionalParameterTable") {
            data[key].forEach(element => {
                acc.push(element.fallbackOptionalParameter + "=" + encodeUriComponent(element.fallbackOptionalValue));
            });
        }
        else {
            acc.push(key + "=" + encodeUriComponent(data[key]));
        }
      
        return acc;
    }, [])
    .join("&");
// Generate lambda URL
const lambdaURL = "https://gtm-script-generator.voluum.codewise.com/script?" + queryParams;

// Log output lambda url
logToConsole(lambdaURL);

// Inject script from lambda
injectScript(lambdaURL, data.gtmOnSuccess, data.gtmOnFailure);


